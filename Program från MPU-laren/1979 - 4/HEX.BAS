10 REM PROGRAM F\R ATT OMVANDLA
15 REM HEX TILL DECIMALT
20 REM T.EX.000F=15
40 LET A$=" 0123456789ABCDEF"
50 INPUT "HEX",B$
60 FOR E=1 TO 17
70 IF LEFT$(B$,1)=MID$(A$,E,1) THEN A=E-2
80 IF MID$(B$,2,1)=MID$(A$,E,1) THEN B=E-2
90 IF MID$(B$,3,1)=MID$(A$,E,1) THEN C=E-2
100 IF RIGHT$(B$,1)=MID$(A$,E,1) THEN D=E-2
110 NEXT E
120 LET F=A*4096+B*256+C*16+D
130 PRINT
140 PRINT"DECIMALT ";F
150 END
